<tr>
  <td>
      <%= assignment.name %>
  </td>
  <td>
      <%= assignment.created_at %>
  </td>
  <td>
      <%= assignment.updated_at %>
  </td>
  <td>
    <%= link_to 'View', assignment, id: assignment.id %>

    <% if teacher_user? %>
      <%= link_to 'Edit', controller: "assignments", action: "edit", id: assignment.id  %>
      <%= link_to 'Delete', assignment, :method => "delete", :confirm=>"Are you sure?" %>
    <% end %>
  </td>
</tr>
