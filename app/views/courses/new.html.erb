<h1>New Course</h1>
<%= form_with(url: courses_create_path, model: @course, local: true) do |f| %>
  <table>
    <tr>
      <th><%= f.label :name %></th>
      <td>:</td>
      <td><%= f.text_field :name%></td>
    </tr>

    <tr>
      <th><%= f.hidden_field :teacher_id %></th>
      <td></td>
      <td><%= f.submit "Create"%></td>

    </tr>
  </table>

<% end %>
