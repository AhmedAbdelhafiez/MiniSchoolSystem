<h1>Edit Course</h1>
<%= form_with(url: courses_update_path, model: @course, local: true) do |f| %>
<table>
  <tr>
    <th><%= f.label :name %></th>
    <td>:</td>
    <td><%= f.text_field :name%></td>
  </tr>
  <tr>
    <th><%= f.hidden_field :id %></td>
      <td></td>
    <td><%= f.submit "Update"%></td>
  </tr>
</table>
<% end %>
